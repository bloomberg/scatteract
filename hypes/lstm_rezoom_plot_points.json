{
    "data": {
        "train_idl": "./data/plots_v1/train_points.idl",
        "test_idl": "./data/plots_v1/test_points.idl",
        "truncate_data": false
    },
    "logging": {
        "display_iter": 50,
        "save_iter": 25000
    },
    "solver": {
        "opt": "RMS",
        "use_jitter": false,
        "rnd_seed": 1,
        "epsilon": 0.00001, 
        "learning_rate": 0.002, 
        "learning_rate_step": 20000, 
        "hungarian_iou": 0.25,
        "weights": "",
        "head_weights": [1.0, 0.1],
	"max_iter": 125001
    },
    "use_lstm": true,
    "use_rezoom": true,
    "biggest_box_px": 10000,
    "rezoom_change_loss": "center",
    "rezoom_w_coords": [-0.25, 0.25],
    "rezoom_h_coords": [-0.25, 0.25],
    "reregress": true,
    "focus_size": 1.8,
    "early_feat_channels": 256,
    "avg_pool_size": 5,
    "num_lstm_layers": 2,
    "image_width": 1440, 
    "image_height": 1440,
    "grid_height": 45, 
    "grid_width": 45,
    "batch_size": 1, 
    "region_size": 32,
    "clip_norm": 1.0,
    "lstm_size": 500,
    "deconv": false,
    "num_classes": 2,
    "rnn_len": 15
}
